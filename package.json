{
  "name": "babel-convert-jsy-from-js",
  "version": "0.0.1",
  "description": "Convert JavaScript, Babel or Babylon AST into offside indented JSY formatted source code.",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "license": "BSD-2-Clause",
  "keywords": [
    "babel",
    "syntax",
    "offside",
    "python",
    "indention",
    "indent",
    "jsy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanewholloway/babel-convert-jsy-from-js"
  },
  "bugs": {
    "url": "https://github.com/shanewholloway/babel-convert-jsy-from-js/issues"
  },
  "homepage": "https://github.com/shanewholloway/babel-convert-jsy-from-js#readme",
  "files": [
    "code",
    "dist"
  ],
  "bin": {
    "jsy-from-js": "dist/jsy-from-js.js"
  },
  "main": "dist/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-generator": "^6.26.0",
    "babel-preset-jsy": "^0.9.2",
    "babel-cli": "^6.26.0"
  },
  "babel": {
    "presets": [
      "jsy/lean"
    ]
  },
  "scripts": {
    "build": "babel -x .js,.jsy code -d dist",
    "watch": "npm -s run build -- --watch",
    "pretest": "npm -s run build",
    "test": "true"
  }
}
